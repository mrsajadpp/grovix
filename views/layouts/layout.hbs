<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="description" content="{{description}}">
  <meta name="og:description" content="{{description}}">
  <meta name="keywords" content="{{keywords}}">
  <meta name="title" content="{{title}}">
  <meta name="og:title" content="{{title}}">
  <meta property="site_name" content="Elegent Purse" />
  <meta property="og:site_name" content="Elegent Purse" />
  <meta name="theme-color" content="#6f4e37">
  <meta name="robots" content="follow, index">
  {{#if author}}
  <meta name="author" content="{{author}}">
  {{/if}}
  {{#if image}}
  <meta name="og:image" content="http://elegentpurse.com/images/{{image}}.jpeg">
  <meta property="og:image:secure_url" content="https://elegentpurse.com/images/{{image}}.jpeg" />
  {{else}}
  <meta name="og:image" content="http://elegentpurse.com/images/opengraph.png">
  <meta property="og:image:secure_url" content="https://elegentpurse.com/images/opengraph.png" />
  {{/if}}
  {{#if product}}
  <meta property="og:type" content="product" />
  {{else}}
  <link rel="canonical" href="https://www.elegentpurse.com/">
  {{/if}}
  <link rel="icon" type="image/x-icon" href="/images/favicon.ico" />
  <link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon-180x180.png">
  <link rel="apple-touch-icon" sizes="152x152" href="/images/apple-touch-icon-152x152.png">
  <link rel="apple-touch-icon" sizes="120x120" href="/images/apple-touch-icon-120x120.png">

  <title>{{title}}</title>
  {{!--
  <link rel="stylesheet" href="/stylesheets/style.css"> --}}
  {{#if style}}
  <link rel="stylesheet" href="/stylesheets/{{style}}.css">
  {{/if}}


  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous" />

  <!-- Custom CSS -->
  <link rel="stylesheet" href="/stylesheets/userstyles.css" />

  <!-- Font Awesome Icons -->
  <script src="https://kit.fontawesome.com/b3d0fe1f35.js" crossorigin="anonymous"></script>
  <link rel="stylesheet"
    href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />

  <!-- Boxicons -->
  <script src="https://unpkg.com/boxicons@2.1.4/dist/boxicons.js"></script>

  <!-- Google Fonts  -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link
    href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
    rel="stylesheet">

  <style>
    a {
      color: #000;
      text-decoration: none;
    }

    input {
      outline: none;
    }

    button {
      outline: none;
    }
  </style>

  <!-- Google tag (gtag.js) -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=G-NQJXWQQT1Z"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag() { dataLayer.push(arguments); }
    gtag('js', new Date());

    gtag('config', 'G-NQJXWQQT1Z');
  </script>

  <!-- Google Tag Manager -->
  <script>(function (w, d, s, l, i) {
      w[l] = w[l] || []; w[l].push({
        'gtm.start':
          new Date().getTime(), event: 'gtm.js'
      }); var f = d.getElementsByTagName(s)[0],
        j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src =
          'https://www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f);
    })(window, document, 'script', 'dataLayer', 'GTM-KCPPWZ7R');</script>
  <!-- End Google Tag Manager -->
</head>

<body>
  <!-- Google Tag Manager (noscript) -->
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-KCPPWZ7R" height="0" width="0"
      style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->

  {{#if product}}
  <!-- Add structured data for the product -->
  <script type="application/ld+json">
    {
        "@context": "http://schema.org/",
        "@type": "Product",
        "name": "{{product.name}}",
        "description": "{{product.description}}",
        "image": "http://elegentpurse.com/images/{{product._id}}.jpeg",
        "sku": "{{product.SKU}}",
        "brand": {
            "@type": "Brand",
            "name": "Elegent Purse"
        },
        "offers": {
            "@type": "Offer",
            "priceCurrency": "INR",
            "price": "{{product.sale_price}}",
            "availability": "http://schema.org/InStock"
        },
        "aggregateRating": {
            "@type": "AggregateRating",
            "ratingValue": "4.5",
            "reviewCount": "20"
        },
        "review": [
            {
                "@type": "Review",
                "reviewRating": {
                    "@type": "Rating",
                    "ratingValue": "4",
                    "bestRating": "5",
                    "worstRating": "1"
                },
                "author": {
                    "@type": "User",
                    "name": "Sajad PP"
                },
                "datePublished": "2024-03-30",
                "reviewBody": "I love this product. It's great!"
            }
        ],
        "url": "http://elegentpurse.com/product/{{product._id}}"
    }
    </script>
  {{/if}}
  {{#if auth}}
  {{else}}
  {{> header user=user}}
  {{/if}}
  {{{body}}}
  {{> footer}}
  <!-- jQuery -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
  <!-- Bootstrap Bundle JS -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.3.3/js/bootstrap.bundle.min.js"></script>
</body>

</html>